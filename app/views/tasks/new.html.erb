<h1>New Task</h1>

<%= form_for(@task) do |t| %>
<p>
  <%= t.label :title %>
  <%= t.text_field :title %>
</p>
<p>

  <%= t.label :details %>
  <%= t.text_field :details %>
</p>
<p>
  <%= t.submit %>
</p>
<% end %>




<%= link_to "http://google.com" do %>
  <i class='far fa-trash-alt'></i>
<% end %>





